{
  "name": "zebra-2",
  "version": "1.0.0",
  "description": "zebra",
  "author": "wuyou",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "lint": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "dev-debug": "node --inspect node_modules/.bin/nuxt"
  },
  "dependencies": {
    "@nestjs/common": "^5.4.0",
    "@nestjs/core": "^5.4.0",
    "@nestjs/typeorm": "^5.2.2",
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/dotenv": "^1.2.1",
    "@types/lodash": "^4.14.118",
    "@types/mysql": "^2.15.5",
    "@types/reflect-metadata": "^0.1.0",
    "cross-env": "^5.2.0",
    "element-ui": "^2.4.6",
    "express": "^4.16.3",
    "lodash": "^4.17.11",
    "mysql": "^2.16.0",
    "nestjs-extensions": "^0.29.0",
    "nuxt": "^2.0.0",
    "nuxt-class-component": "^1.2.1",
    "nuxt-typescript": "^0.11.0",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.3.3",
    "ts-loader": "^5.3.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.16.0",
    "typeorm": "^0.2.9"
  },
  "devDependencies": {
    "@types/express-session": "^1.15.11",
    "@types/mime-types": "^2.1.0",
    "@types/multer": "^1.3.7",
    "@types/node": "^10.12.9",
    "nodemon": "^1.11.0",
    "prettier": "1.14.3",
    "ts-loader": "^3.5.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.1.6",
    "typescript-eslint-parser": "^20.1.1"
  },
  "nodemonConfig": {
    "watch": [
      "server",
      "nuxt.config.js"
    ],
    "ext": "js ts",
    "ignore": [
      "server/**/*.spec.ts",
      "server/build/"
    ],
    "exec": "ts-node --type-check --project server server"
  }
}
